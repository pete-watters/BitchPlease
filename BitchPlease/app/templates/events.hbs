<!-- Main -->
<div id="main-wrapper">
    <div class="container">
      <div class="row">
                <div class="12u">
                <header class="major">
                  <h2>Upcoming Events</h2>
                </header>
                <ul class="dates">
                {{#each}}
                  <li>
                    <span class="date"><strong>{{post_date}}</strong></span>
                    <h3><a href="#">{{post_title}}</a></h3>
                    <p>{{{post_body}}}</p>
                 </li>
                {{/each}}
                </ul>

          </div>
        </div>
    </div>
</div>